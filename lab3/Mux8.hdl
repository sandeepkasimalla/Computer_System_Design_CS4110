CHIP Mux8 {
    IN ip1[8], ip2[8], s;
    OUT out[8];

    PARTS:
    Mux(ip1=ip1[0], ip2=ip2[0], s=s, out=out[0]);
    Mux(ip1=ip1[1], ip2=ip2[1], s=s, out=out[1]);
    Mux(ip1=ip1[2], ip2=ip2[2], s=s, out=out[2]);
    Mux(ip1=ip1[3], ip2=ip2[3], s=s, out=out[3]);
    Mux(ip1=ip1[4], ip2=ip2[4], s=s, out=out[4]);
    Mux(ip1=ip1[5], ip2=ip2[5], s=s, out=out[5]);
    Mux(ip1=ip1[6], ip2=ip2[6], s=s, out=out[6]);
    Mux(ip1=ip1[7], ip2=ip2[7], s=s, out=out[7]);
}